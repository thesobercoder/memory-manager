{
  "name": "effect-app",
  "dependencies": {
    "@effect/platform": "^0.87.1",
    "@effect/platform-bun": "^0.72.3",
    "effect": "^3.16.10"
  },
  "devDependencies": {
    "@effect/language-service": "^0.23.3",
    "@types/bun": "latest",
    "prettier": "^3.6.2",
    "syncpack": "^13.0.4"
  },
  "module": "index.ts",
  "peerDependencies": {
    "typescript": "^5"
  },
  "private": true,
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target bun --minify",
    "dev": "bun run src/index.ts",
